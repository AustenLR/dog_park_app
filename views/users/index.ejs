<% include ../partials/header %>



<div class="row">
  <% users.forEach(function(el){ %>
  <div class="col-sm-4 col-md-4">
    <div class="thumbnail">
      <img src="<%=el.picture%>">
      <div class="caption">
        <h3> <a href="/users/<%= el.id %>"> <%= el.username %></a></h3>
        <h4> Age: <%= el.age %></a></h3>
        <h4> Breed: <%= el.breed %></a></h3>       
      </div>
    </div>
  </div>
  <%  }) %>
</div>



<% include ../partials/footer %>