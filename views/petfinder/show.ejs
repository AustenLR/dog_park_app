<% include ../partials/header %>
<h1 class='text-center'>Results</h1>
<div class="row">
  <% pets.forEach(function(pet){ %>
    <div class="col-xs-6 col-md-4">
      <div class="thumbnail thumbnailPetfinder">
          <% if(pet.media.photos) {%>
            <img src="<%= pet.media.photos.photo[3].$t %>" width='200' height='200' class='img-rounded'>  
          <% } else { %>
          <img src="data:image/jpeg;base64," width='200' height='200' class='img-rounded'>  
          <% } %>
          <div class="caption">
            <h3><%= pet.name.$t %> </h3>
            <h5>Breed: <%= pet.breeds.breed.$t %> </h5>
            <h5>Sex:<%= pet.sex.$t %> </h5>
            <h5>Age:<%= pet.age.$t %> </h5>
            <h5><%= pet.contact.address1.$t %> </h5>
            <h5><%= pet.contact.city.$t %> </h5>
            <h5><%= pet.contact.phone.$t %> </h5>
            <h5><%= pet.contact.email.$t %> </h5>
            <h5>Description:<%= pet.description.$t %> </h5>
          </div>
      </div>
    </div>
  <%  }) %>
</div>



<% include ../partials/footer %>